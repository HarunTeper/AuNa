CompileFlags:
  Add:
    - "-std=c++17"
    - "-I/opt/ros/${ROS_DISTRO}/include"
    - "-I/opt/ros/${ROS_DISTRO}/include/rclcpp"
    - "-I${workspaceFolder}/packages/install/include"
    # libstdc++ for gcc-11
    - "-I/usr/include/c++/11"
    - "-I/usr/include/x86_64-linux-gnu/c++/11"
    # libstdc++ for gcc-12
    - "-I/usr/include/c++/12"
    - "-I/usr/include/x86_64-linux-gnu/c++/12"
  CompilationDatabase: "packages/build"
Index:
  Background: Build