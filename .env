### DYNAMIC PARAMETERS ###

# User configuration
ROS_DOMAIN_ID=0
HOST_UID=1000
HOST_GID=1000

# Simulation scenario parameters
WORLD_NAME=racetrack_decorated
COMMUNICATION_TYPE=cam # cam or omnet
USE_WAYPOINTS=true

### STATIC PARAMETERS ###

# Build parameters
ROS_DISTRO=humble
RMW_IMPLEMENTATION=rmw_zenoh_cpp
PIP_BREAK_SYSTEM_PACKAGES=1

# Simulation scenario parameters
MAP_NAME=${WORLD_NAME}

# Tracing Parameters
ROS_TRACE_DIR=/home/ubuntu/workspace/traces

# Package names for physical_car service
# Space-separated list of package names to build
GAZEBO_PACKAGES=auna_gazebo auna_msgs auna_common
TF_PACKAGES=auna_tf
GROUND_TRUTH_PACKAGES=auna_ground_truth
EKF_PACKAGES=auna_ekf
PHYSICAL_CAR_PACKAGES=physical_car hardware_drivers
TELEOP_PACKAGES=auna_teleoperation
CONTROL_PANEL_PACKAGES=auna_control auna_msgs
COMM_PACKAGES=auna_comm
NAV2_PACKAGES=auna_nav2 auna_common
CACC_PACKAGES=auna_cacc auna_msgs auna_common
WALLFOLLOWING_PACKAGES=auna_wallfollowing
WAYPOINT_PUBLISHER_PACKAGES=auna_waypoints

# Extra packages
DEMO_PACKAGES=ros-humble-demo-nodes-cpp
ZENOHD_PACKAGES=ros-humble-rmw-zenoh-cpp
TF2_TOOLS_PACKAGES=ros-humble-tf2-tools
RQT_PACKAGES=ros-humble-rqt*